public class Pyramid12345 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i=1;i<=5;i++)		
		{
				int j=i,k=1;
			
				while(j>0)
				{
				System.out.print(k);
				j--;k++;
				}
			System.out.println();
		}
			for(int i=4;i>=1;i--)
			{
					int j=i,k=1;
				
					while(j>0)
					{
					System.out.print(k);
					j--;k++;
					}
				System.out.println();
			}
	}
}
